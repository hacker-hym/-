<view class="page">
  <view class="page-bottom">
    <view class="page-content">
      <view class="wc">
        <navigator url="../one/index" hover-class="navigator-hover">环保随行</navigator>
      </view>
      <view class="wc">
        <navigator url="../two/index" hover-class="navigator-hover">守护朋友</navigator>
      </view>
      <view class="wc">
        <navigator url="../three/index" hover-class="navigator-hover">点滴环保</navigator>
      </view>
      <view class="wc">
        <navigator url="../four/index" hover-class="navigator-hover">全民动员</navigator>
      </view>
      <view class="wc">
        <navigator url="../five/index" hover-class="navigator-hover">社交讨论</navigator>
      </view>
      <view class="wc">
        <navigator url="../six/index" hover-class="navigator-hover">关于我们</navigator>
      </view>
      <view class="wc">
        <navigator url="/pages/homePage/homePage" hover-class="navigator-hover" open-type="switchTab">回到首页</navigator>
      </view>
    </view>
  </view>
  <view class="page-top {{open ? 'c-state2' : ''}}">
    <image bindtap="tap_ch" src="/images/tabbar/menusButton.png" class="page-top-image"></image>
    <view class="text title">社交讨论</view>
    <view class="class1" wx:for="{{positionList}}">
      <navigator url="{{item.url}}">
        <view class="class2 horizontalArrangement">
          <image src="{{item.imagePath}}" class="class3"></image>
          <view class="verticalAlign class4">
            <text class="title">{{item.title}}</text>
            <text class="annotation">{{item.introduct}}</text>
          </view>
        </view>
      </navigator>
    </view>
  </view>
</view>